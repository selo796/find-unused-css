{
  "name": "find-unused-css",
  "description": "Find unused css selectors in your project",
  "version": "0.5.0",
  "author": "Selcuk Imal <selcuk.imal.dev@gmail.com>",
  "homepage": "https://github.com/selo796/find-unused-css",
  "bugs": "https://github.com/selo796/find-unused-css/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/selo796/find-unused-css"
  },
  "license": "MIT",
  "keywords": [
    "unusedcss",
    "find",
    "search",
    "analyze",
    "findcss",
    "css"
  ],
  "engines": {
    "node": ">= 0.12.0"
  },
  "main": "./src/main.js",
  "preferGlobal": true,
  "bin": {
    "find-unused-css": "./bin/findUnusedCss"
  },
  "scripts": {
    "start": "node src/main.js",
    "test": "istanbul cover jasmine-node -- specs",
    "unit": "./node_modules/.bin/jasmine-node specs/ --color --autotest --watch ./",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "cli-spinner": "^0.2.5",
    "glob": "^7.0.6",
    "htmlparser2": "^3.9.0",
    "line-by-line": "^0.1.4"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "jasmine-es6": "^0.1.10",
    "jasmine-node": "^1.14.5"
  },
  "files": [
    "bin",
    "src"
  ]
}
